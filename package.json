{
  "name": "gamefaqs-reader",
  "version": "0.1.0",
  "description": "A react app that reads gamefaqs guides",
  "author": "Andrew McOlash",
  "license": "MIT",
  "main": "build/dist-electron/main.js",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build && electron-builder"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.5",
    "electron-fetch": "^1.9.1",
    "electron-window-state": "^5.0.3",
    "joypad.js": "^2.3.3",
    "jsdom": "^20.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.18.0",
    "react-simple-keyboard": "^3.4.252",
    "set-cookie-parser": "^2.5.1",
    "simple-keyboard-key-navigation": "^2.4.246",
    "typestyle": "^2.4.0"
  },
  "devDependencies": {
    "babel-preset-nano-react-app": "^0.1.0",
    "electron": "^23.1.3",
    "electron-builder": "^24.0.0",
    "vite": "^3.2.4",
    "vite-electron-plugin": "^0.8.2",
    "vite-plugin-svgr": "^2.2.2"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ]
  },
  "build": {
    "appId": "com.amcolash.gamefaqs-reader",
    "asar": true,
    "directories": {
      "output": "build/release/${version}"
    },
    "files": [
      "build/dist",
      "build/dist-electron"
    ],
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
